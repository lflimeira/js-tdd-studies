{
  "name": "tdd",
  "version": "1.0.0",
  "description": "Biblioteca TDD",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --colors --progress",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "server": "./node_modules/.bin/webpack-dev-server --inline --open"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": ["text","html"],
    "exclude": ["tests/**"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lflimeira/js-tdd-studies.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "lflimeira <lflimeira02@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lflimeira/js-tdd-studies/issues"
  },
  "homepage": "https://github.com/lflimeira/js-tdd-studies#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  }
}
